import discord
from discord.ext import commands
import os
import random

class Fun(commands.Cog):

    def __init__(self, bot):
        self.bot = bot

        @commands.command()
        async def example(self,ctx):
            await ctx.send('cog command')
            


def setup(bot):
    bot.add_cog(Fun(bot))   
    print('Fun is loaded')
